{
    "manifest_version" : 2,
    "name" : "Flickr Exposition",
    "version" : "1.3.3",
    "description" : "Simple flickr exposition extension.",
    "icons" : {
        "16" : "resources/images/flickr-blue-16.png",
        "48" : "resources/images/flickr-blue-48.png",
        "128" : "resources/images/flickr-blue-128.png",
        "256" : "resources/images/flickr-blue-256.png"
    },
    "browser_action" : {
        "default_icon" : "resources/images/flickr-icon.png",
        "default_popup" : "popup.html"
    },
    "permissions" : ["https://*.flickr.com/", "tabs", "notifications"]
}
